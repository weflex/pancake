    "orgmode": "2.1.1",
    "parsimmon": "0.7.0"
  },
  "devDependencies": {
    "tape": "4.5.1"
  },
  "firedoc": {
    "linkNatives": true,
    "paths": "./lib",
    "dest": "./docs"
  }
}
